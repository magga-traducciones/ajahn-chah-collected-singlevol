(indexentry :tkey (("vedana" "\\glossaryentryfield{vedana}{\\glsnamefont{vedan\\=a}}{Feeling. Either painful (\\protect \\textit  {dukkha-}), pleasant (\\protect \\textit  {sukha-}), or neither-painful-nor-pleasant (\\protect \\textit  {adukkha\\d {m}-asukh\\=a}). \\protect \\seepre \\protect \\glsname {khandha}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("brahmana" "\\glossaryentryfield{brahmana}{\\glsnamefont{br\\=ahma\\.na}}{The \\protect \\textit  {br\\=ahman} caste of India has long maintained that its members, by their birth, are worthy of the highest respect. Buddhism borrowed the term \\protect \\textit  {br\\=ahman} to apply to those who have attained the goal, to show that respect is earned not by birth, race, or caste, but by spiritual attainment. In the Buddhist sense, synonymous with \\protect \\textit  {arahant}.}{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("upasampada" "\\glossaryentryfield{upasampada}{\\glsnamefont{upasampad\\=a}}{Acceptance; full ordination as a \\protect \\textit  {bhikkhu} or \\protect \\textit  {bhikkhun\\={\\i }}. \\protect \\seepre \\protect \\glsname {pabbajja}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("parisa" "\\glossaryentryfield{parisa}{\\glsnamefont{paris\\=a}}{Following; assembly. The four groups of the Buddha's following that include monks, nuns, laymen, and laywomen. \\protect \\seepre \\protect \\glsname {sangha}, \\protect \\glsname {bhikkhu}, \\protect \\glsname {bhikkhuni}, \\protect \\glsname {upasaka}, \\protect \\glsname {upasika}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("sangha" "\\glossaryentryfield{sangha}{\\glsnamefont{sa\\.ngha}}{On the conventional (\\protect \\textit  {sammuti}) level, this term denotes the communities of Buddhist monks and nuns; on the ideal (\\protect \\textit  {ariya}) level, it denotes those followers of the Buddha, lay or ordained, who have attained at least stream-entry (\\protect \\textit  {sot\\=apanna}), the first of the transcendent paths (\\protect \\textit  {magga}) culminating in nibb\\=ana. \\protect \\seepre \\protect \\glsname {sotapanna}, \\protect \\glsname {magga}, \\protect \\glsname {nibbana}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("moha" "\\glossaryentryfield{moha}{\\glsnamefont{moha}}{Delusion; ignorance (\\protect \\textit  {avijj\\=a}). One of three unwholesome roots (\\protect \\textit  {m\\=ula}) in the mind. \\protect \\seepre \\protect \\glsname {mula}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("kayagata-sati" "\\glossaryentryfield{kayagata-sati}{\\glsnamefont{k\\=ayagat\\=a-sati}}{Mindfulness immersed in the body. This is a blanket term covering several meditation themes: keeping the breath in mind; being mindful of the body's posture; being mindful of one's activities; analyzing the body into its parts; analyzing the body into its physical properties (\\protect \\textit  {dh\\=atu}); contemplating the fact that the body is inevitably subject to death and disintegration. \\protect \\seepre \\protect \\glsname {dhatu}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("samyojana" "\\glossaryentryfield{samyojana}{\\glsnamefont{sa\\d {m}yojana}}{Fetter that binds the mind to the cycle of rebirth (\\protect \\textit  {va\\d {t}\\d {t}a}) -- self-identification views (\\protect \\textit  {sakk\\=aya-di\\d {t}\\d {t}hi}), uncertainty (\\protect \\textit  {vicikicch\\=a}), grasping at precepts and practices (\\protect \\textit  {s\\={\\i }labbata-par\\=am\\=asa}); sensual passion (\\protect \\textit  {k\\=ama-r\\=aga}), aversion (\\protect \\textit  {vy\\=ap\\=ada}); passion for form (\\protect \\textit  {r\\=upa-r\\=aga}), passion for formless phenomena (\\protect \\textit  {ar\\=upa-r\\=aga}), conceit (\\protect \\textit  {m\\=ana}), restlessness (\\protect \\textit  {uddhacca}), and unawareness (\\protect \\textit  {avijj\\=a}). \\protect \\seepre \\protect \\glsname {vatta}, \\protect \\glsname {samsara}, \\protect \\glsname {anusaya}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("ajahn" "\\glossaryentryfield{ajahn}{\\glsnamefont{Ajahn}}{(\\protect \\textit  {Thai}) From the P\\=a\\d {l}i \\protect \\textit  {\\=acariya}, literally `teacher'; often used as a title of senior monks or nuns of more than ten years' seniority in a monastery.}{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("anusaya" "\\glossaryentryfield{anusaya}{\\glsnamefont{anusaya}}{Predisposition; underlying tendency. There are seven major underlying tendencies to which the mind returns over and over again: tendency towards sensual passion (\\protect \\textit  {k\\=ama-r\\=aganusaya}), aversion (\\protect \\textit  {pa\\d {t}i\\protect \\-gh\\=a\\protect \\-nu\\protect \\-saya}), views (\\protect \\textit  {di\\d {t}\\d {t}hanusaya}), uncertainty (\\protect \\textit  {vicikicchanusaya}), conceit (\\protect \\textit  {m\\=a\\protect \\-nu\\protect \\-saya}), passion for becoming (\\protect \\textit  {bhava-r\\=aganusaya}), and towards \\unhbox \\voidb@x \\hbox {ignorance} (\\protect \\textit  {avijj\\=a\\protect \\-nu\\protect \\-saya}). \\protect \\seepre \\protect \\glsname {samyojana}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("upasika" "\\glossaryentryfield{upasika}{\\glsnamefont{up\\=asik\\=a}}{A lay devotee (female).}{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("upasaka" "\\glossaryentryfield{upasaka}{\\glsnamefont{up\\=asaka}}{A lay devotee (male).}{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("bhikkhuni" "\\glossaryentryfield{bhikkhuni}{\\glsnamefont{bhikkhun\\={\\i }}}{A Buddhist nun; a woman who has given up the householder's life to live a life of heightened virtue (\\protect \\textit  {s\\={\\i }la}) in accordance with the \\protect \\textit  {Vinaya} in general, and the \\protect \\textit  {P\\=atimokkha} rules in particular. \\protect \\seepre \\protect \\glsname {sila}, \\protect \\glsname {vinaya}, \\protect \\glsname {patimokkha}, \\protect \\glsname {sangha}, \\protect \\glsname {parisa}, \\protect \\glsname {upasampada}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("mula" "\\glossaryentryfield{mula}{\\glsnamefont{m\\=ula}}{Literally, `root'. The fundamental conditions in the mind that determine the moral quality -- skillful (\\protect \\textit  {kusala}) or unskillful (\\protect \\textit  {akusala}) -- of one's intentional actions (\\protect \\textit  {kamma}). The three unskillful roots are \\protect \\textit  {lobha} (greed), \\protect \\textit  {dosa} (aversion), and \\protect \\textit  {moha} (delusion); the skillful roots are their opposites. \\protect \\seepre \\protect \\glsname {kusala}, \\protect \\glsname {kamma}, \\protect \\glsname {kilesa}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("saddha" "\\glossaryentryfield{saddha}{\\glsnamefont{saddh\\=a}}{Conviction, faith, trust. A confidence in the Buddha that gives one the willingness to put his teachings into practice. Conviction becomes unshakeable upon the attainment of stream-entry. \\protect \\seepre \\protect \\glsname {sotapanna}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("indriya" "\\glossaryentryfield{indriya}{\\glsnamefont{indriya}}{Spiritual faculties; mental factors. In the suttas the term can refer either to the six sense media (\\protect \\textit  {\\=ayatana}) or to the five mental factors of \\protect \\textit  {saddh\\=a} (conviction), \\protect \\textit  {viriya} (persistence), \\protect \\textit  {sati} (mindfulness), \\protect \\textit  {sam\\=adhi} (concentration), and \\protect \\textit  {pa\\~n\\~n\\=a} (discernment). \\protect \\seepre \\protect \\glsname {bodhi-pakkhiya-dhamma}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsgobblenumber" ) 
(indexentry :tkey (("ajahn" "\\glossaryentryfield{ajahn}{\\glsnamefont{Ajahn}}{(\\protect \\textit  {Thai}) From the P\\=a\\d {l}i \\protect \\textit  {\\=acariya}, literally `teacher'; often used as a title of senior monks or nuns of more than ten years' seniority in a monastery.}{\\relax }") ) :locref "{}{751}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("kalyanamitta" "\\glossaryentryfield{kalyanamitta}{\\glsnamefont{kaly\\=a\\d {n}amitta}}{Noble friend; a mentor or teacher of Dhamma.}{\\relax }") ) :locref "{}{751}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("parami" "\\glossaryentryfield{parami}{\\glsnamefont{p\\=aram\\={\\i }}}{(Skt. \\protect \\textit  {p\\=aramit\\=a}) Perfection of the character. A group of ten qualities developed over many lifetimes by a \\protect \\textit  {bodhisatta}: generosity (\\protect \\textit  {d\\=ana}), virtue (\\protect \\textit  {s\\={\\i }la}), renunciation (\\protect \\textit  {nekkhamma}), discernment (\\protect \\textit  {pa\\~n\\~n\\=a}), energy / persistence (\\protect \\textit  {viriya}), patience / forbearance (\\protect \\textit  {khanti}), truthfulness (\\protect \\textit  {sacca}), determination (\\protect \\textit  {adhi\\d {t}\\d {t}h\\=ana}), good will (\\protect \\textit  {mett\\=a}) and equanimity (\\protect \\textit  {upekkh\\=a}). \\protect \\seepre \\protect \\glsname {bodhisatta}, \\protect \\glsname {dana}, \\protect \\glsname {sila}, \\protect \\glsname {nekkhamma}, \\protect \\glsname {panna}, \\protect \\glsname {viriya}, \\protect \\glsname {khanti}, \\protect \\glsname {sacca}, \\protect \\glsname {adhitthana}, \\protect \\glsname {metta}, \\protect \\glsname {upekkha}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("kusala" "\\glossaryentryfield{kusala}{\\glsnamefont{kusala}}{Wholesome, skillful, good, meritorious. An action characterized by this moral quality (\\protect \\textit  {kusala-kamma}) is bound to result (eventually) in happiness and a favorable outcome. Actions characterized by its opposite (\\protect \\textit  {akusala-kamma}) lead to sorrow. \\protect \\seepre \\protect \\glsname {kamma}\\protect \\seepost }{\\relax }") ) :locref "{}{751}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("samyojana" "\\glossaryentryfield{samyojana}{\\glsnamefont{sa\\d {m}yojana}}{Fetter that binds the mind to the cycle of rebirth (\\protect \\textit  {va\\d {t}\\d {t}a}) -- self-identification views (\\protect \\textit  {sakk\\=aya-di\\d {t}\\d {t}hi}), uncertainty (\\protect \\textit  {vicikicch\\=a}), grasping at precepts and practices (\\protect \\textit  {s\\={\\i }labbata-par\\=am\\=asa}); sensual passion (\\protect \\textit  {k\\=ama-r\\=aga}), aversion (\\protect \\textit  {vy\\=ap\\=ada}); passion for form (\\protect \\textit  {r\\=upa-r\\=aga}), passion for formless phenomena (\\protect \\textit  {ar\\=upa-r\\=aga}), conceit (\\protect \\textit  {m\\=ana}), restlessness (\\protect \\textit  {uddhacca}), and unawareness (\\protect \\textit  {avijj\\=a}). \\protect \\seepre \\protect \\glsname {vatta}, \\protect \\glsname {samsara}, \\protect \\glsname {anusaya}\\protect \\seepost }{\\relax }") ) :locref "{}{752}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("nibbana" "\\glossaryentryfield{nibbana}{\\glsnamefont{nibb\\=ana}}{(Skt. \\protect \\textit  {nirv\\=ana}) Final liberation from all suffering, the goal of Buddhist practice. The liberation of the mind from the mental effluents (\\protect \\textit  {\\=asava}), defilements (\\protect \\textit  {kilesa}), and the round of rebirth (\\protect \\textit  {va\\d {t}\\d {t}a}), and from all that can be described or defined. As this term also denotes the extinguishing of a fire, it carries the connotations of stilling, cooling, and peace. (According to the physics taught at the time of the Buddha, a burning fire seizes or adheres to its fuel; when extinguished, it is unbound.) `Total nibb\\=ana' in some contexts denotes the experience of Awakening; in others, the final passing away of an \\protect \\textit  {arahant}. \\protect \\seepre \\protect \\glsname {asava}, \\protect \\glsname {kilesa}, \\protect \\glsname {arahant}, \\protect \\glsname {vatta}\\protect \\seepost }{\\relax }") ) :locref "{}{752}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("dana" "\\glossaryentryfield{dana}{\\glsnamefont{d\\=ana}}{Giving, liberality; offering, alms. Specifically, giving of any of the four requisites to the monastic order. More generally, the inclination to give, without expecting any form of repayment from the recipient. \\protect \\textit  {D\\=ana} is the first theme in the Buddha's system of gradual training (\\protect \\textit  {\\=anupubb\\={\\i }-kath\\=a}), the first of the ten \\protect \\textit  {p\\=aram\\={\\i }s}, one of the seven treasures (\\protect \\textit  {dhana}), and the first of the three grounds for meritorious action (\\protect \\textit  {s\\={\\i }la} and \\protect \\textit  {bh\\=avan\\=a}). \\protect \\seepre \\protect \\glsname {anupubbi-katha}, \\protect \\glsname {parami}, \\protect \\glsname {dhana}, \\protect \\glsname {sila}, \\protect \\glsname {bhavana}\\protect \\seepost }{\\relax }") ) :locref "{}{752}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("sila" "\\glossaryentryfield{sila}{\\glsnamefont{s\\={\\i }la}}{Virtue, morality. The quality of ethical and moral purity that prevents one from unskillful actions. Also, the training precepts that restrain one from performing unskillful actions. \\protect \\textit  {S\\={\\i }la} is the second theme in the gradual training (\\protect \\textit  {\\=anupubb\\={\\i }-kath\\=a}), one of the ten \\protect \\textit  {p\\=aram\\={\\i }s}, the second of the seven treasures (\\protect \\textit  {dhana}), and the first of the three grounds for meritorious action. \\protect \\seepre \\protect \\glsname {anupubbi-katha}, \\protect \\glsname {parami}, \\protect \\glsname {dhana}, \\protect \\glsname {dana}, \\protect \\glsname {bhavana}\\protect \\seepost }{\\relax }") ) :locref "{}{752}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("nekkhamma" "\\glossaryentryfield{nekkhamma}{\\glsnamefont{nekkhamma}}{Renunciation; literally, `freedom from sensual lust'. One of the ten \\protect \\textit  {p\\=aram\\={\\i }s}. \\protect \\seepre \\protect \\glsname {parami}\\protect \\seepost }{\\relax }") ) :locref "{}{752}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("four-noble-truths" "\\glossaryentryfield{four-noble-truths}{\\glsnamefont{Four Noble Truths}}{The first and central teaching of the Buddha about dukkha, its origin, cessation and the path leading towards its cessation. Complete understanding of the Four Noble Truths is equivalent to the attainment of nibb\\=ana.}{\\relax }") ) :locref "{}{752}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("samyojana" "\\glossaryentryfield{samyojana}{\\glsnamefont{sa\\d {m}yojana}}{Fetter that binds the mind to the cycle of rebirth (\\protect \\textit  {va\\d {t}\\d {t}a}) -- self-identification views (\\protect \\textit  {sakk\\=aya-di\\d {t}\\d {t}hi}), uncertainty (\\protect \\textit  {vicikicch\\=a}), grasping at precepts and practices (\\protect \\textit  {s\\={\\i }labbata-par\\=am\\=asa}); sensual passion (\\protect \\textit  {k\\=ama-r\\=aga}), aversion (\\protect \\textit  {vy\\=ap\\=ada}); passion for form (\\protect \\textit  {r\\=upa-r\\=aga}), passion for formless phenomena (\\protect \\textit  {ar\\=upa-r\\=aga}), conceit (\\protect \\textit  {m\\=ana}), restlessness (\\protect \\textit  {uddhacca}), and unawareness (\\protect \\textit  {avijj\\=a}). \\protect \\seepre \\protect \\glsname {vatta}, \\protect \\glsname {samsara}, \\protect \\glsname {anusaya}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("kilesa" "\\glossaryentryfield{kilesa}{\\glsnamefont{kilesa}}{(Skt. \\protect \\textit  {klesha}) Defilement -- \\protect \\textit  {lobha} (passion), \\protect \\textit  {dosa} (aversion), and \\protect \\textit  {moha} (delusion) in their various forms, which include such things as greed, malevolence, anger, rancour, hypocrisy, arrogance, envy, miserliness, dishonesty, boastfulness, obstinacy, violence, pride, conceit, intoxication, and complacency.}{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("asava" "\\glossaryentryfield{asava}{\\glsnamefont{\\=asava}}{Mental effluent, taint, fermentation or outflow. Four qualities that taint the mind: sensuality, views, becoming, and ignorance.}{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("samyojana" "\\glossaryentryfield{samyojana}{\\glsnamefont{sa\\d {m}yojana}}{Fetter that binds the mind to the cycle of rebirth (\\protect \\textit  {va\\d {t}\\d {t}a}) -- self-identification views (\\protect \\textit  {sakk\\=aya-di\\d {t}\\d {t}hi}), uncertainty (\\protect \\textit  {vicikicch\\=a}), grasping at precepts and practices (\\protect \\textit  {s\\={\\i }labbata-par\\=am\\=asa}); sensual passion (\\protect \\textit  {k\\=ama-r\\=aga}), aversion (\\protect \\textit  {vy\\=ap\\=ada}); passion for form (\\protect \\textit  {r\\=upa-r\\=aga}), passion for formless phenomena (\\protect \\textit  {ar\\=upa-r\\=aga}), conceit (\\protect \\textit  {m\\=ana}), restlessness (\\protect \\textit  {uddhacca}), and unawareness (\\protect \\textit  {avijj\\=a}). \\protect \\seepre \\protect \\glsname {vatta}, \\protect \\glsname {samsara}, \\protect \\glsname {anusaya}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("arahant" "\\glossaryentryfield{arahant}{\\glsnamefont{arahant}}{Literally: a `Worthy One'. A person whose mind is free of defilement (\\protect \\textit  {kilesa}), who has abandoned all ten of the fetters (\\protect \\textit  {sa\\d {m}yojana}) that bind the mind to the cycle of rebirth, whose heart is free of mental effluents (\\protect \\textit  {\\=asava}), and who is thus not destined for further rebirth. A title for the Buddha and the highest level of his noble disciples. \\protect \\seepre \\protect \\glsname {kilesa}, \\protect \\glsname {asava}, \\protect \\glsname {samyojana}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("ariya-puggala" "\\glossaryentryfield{ariya-puggala}{\\glsnamefont{ariya-puggala}}{Literally: noble person. An individual who has realized at least the lowest of the four noble paths (\\protect \\textit  {magga}) or their fruitions (\\protect \\textit  {phala}). Compare with \\protect \\textit  {puthujjana} (worldling). \\protect \\seepre \\protect \\glsname {magga}, \\protect \\glsname {phala}, \\protect \\glsname {puthujjana}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("magga" "\\glossaryentryfield{magga}{\\glsnamefont{magga}}{`Path'; Specifically, the path to the cessation of suffering and stress. The four transcendent paths -- or rather, one path with four levels of refinement -- are the path to \\protect \\textit  {stream-entry} (entering the stream to nibb\\=ana, which ensures that one will be reborn at most only seven more times), the path to once-returning, the path to non-returning, and the path to arahantship. \\protect \\seepre \\protect \\glsname {eightfold-path}, \\protect \\glsname {phala}, \\protect \\glsname {nibbana}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("phala" "\\glossaryentryfield{phala}{\\glsnamefont{phala}}{Fruition. Specifically, the fruition of any of the four transcendent paths. \\protect \\seepre \\protect \\glsname {magga}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("puthujjana" "\\glossaryentryfield{puthujjana}{\\glsnamefont{puthujjana}}{One of the many-folk; a `worlding'. An ordinary person who has not yet realized any of the four stages of Awakening. \\protect \\seepre \\protect \\glsname {ariya-puggala}, \\protect \\glsname {magga}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("four-noble-truths" "\\glossaryentryfield{four-noble-truths}{\\glsnamefont{Four Noble Truths}}{The first and central teaching of the Buddha about dukkha, its origin, cessation and the path leading towards its cessation. Complete understanding of the Four Noble Truths is equivalent to the attainment of nibb\\=ana.}{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("arahant" "\\glossaryentryfield{arahant}{\\glsnamefont{arahant}}{Literally: a `Worthy One'. A person whose mind is free of defilement (\\protect \\textit  {kilesa}), who has abandoned all ten of the fetters (\\protect \\textit  {sa\\d {m}yojana}) that bind the mind to the cycle of rebirth, whose heart is free of mental effluents (\\protect \\textit  {\\=asava}), and who is thus not destined for further rebirth. A title for the Buddha and the highest level of his noble disciples. \\protect \\seepre \\protect \\glsname {kilesa}, \\protect \\glsname {asava}, \\protect \\glsname {samyojana}\\protect \\seepost }{\\relax }") ) :locref "{}{753}" :attr "pageglsnumberformat" ) 
(indexentry :tkey (("kayagata-sati" "\\glossaryentryfield{kayagata-sati}{\\glsnamefont{k\\=ayagat\\=a-sati}}{Mindfulness immersed in the body. This is a blanket term covering several meditation themes: keeping the breath in mind; being mindful of the body's posture; being mindful of one's activities; analyzing the body into its parts; analyzing the body into its physical properties (\\protect \\textit  {dh\\=atu}); contemplating the fact that the body is inevitably subject to death and disintegration. \\protect \\seepre \\protect \\glsname {dhatu}\\protect \\seepost }{\\relax }") ) :locref "{}{754}" :attr "pageglsnumberformat" ) 

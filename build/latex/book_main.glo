(indexentry :tkey (("bojjhanga" "\\glossaryentryfield{bojjhanga}{\\glsnamefont{bojjha\\.nga}}{The Seven Factors of Enlightenment: mindfulness (\\protect \\textit  {sati}), investigation of Dhamma (\\protect \\textit  {dhamma-vicaya}), energy (\\protect \\textit  {viriya}), rapture (\\protect \\textit  {p\\={\\i }ti}), tranquillity (\\protect \\textit  {passadhi}), concentration or collectedness (\\protect \\textit  {sam\\=adhi}) and equanimity (\\protect \\textit  {upekkh\\=a}).}{\\relax }") ) :locref "{}{755}" :attr "pageglsnumberformat" ) 
